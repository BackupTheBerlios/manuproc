13	Zusammenfügen	\N	\N
31	Verarbeitung	Irgendetwas	\N
2	Äquivalenz	Gleichheit	\N
\.
--
--
--

CREATE UNIQUE INDEX prozesse_prozessid_key ON prozesse USING btree (prozessid);

