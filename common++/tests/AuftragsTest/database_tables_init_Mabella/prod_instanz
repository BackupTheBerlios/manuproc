CREATE TABLE prod_instanz (
    insid numeric(2,0),
    name character varying(20),
    lager_fuer integer,
    sortierung integer,
    lieferschein boolean,
    lagername text,
    produziert_selbst boolean,
    prod_dauer_tage integer,
    einlagern_in numeric(2,0),
    automatisch_einlagern boolean,
    typ character(1),
    externe_bestellung boolean,
    geplant_von integer
);


