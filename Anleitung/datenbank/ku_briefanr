CREATE CONSTRAINT TRIGGER "<unnamed>" AFTER DELETE ON "ku_briefanr"  NOT DEFERRABLE INITIALLY IMMEDIATE FOR EACH ROW EXECUTE PROCEDURE "RI_FKey_noaction_del" ('<unnamed>', 'ku_person', 'ku_briefanr', 'UNSPECIFIED', 'branr', 'branr');

