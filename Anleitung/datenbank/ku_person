CREATE TABLE "ku_person" (
	"persnr" numeric(5,0) NOT NULL,
	"name" character varying(30) NOT NULL,
	"vname" character varying(30),
	"gebdat" date,
	"branr" numeric(1,0),
	"notiz" character varying(500),
	Constraint "ku_person_pkey" Primary Key ("persnr")
);

